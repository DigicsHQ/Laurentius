<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://laurentius.si/process" targetNamespace="http://laurentius.si/process" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="SEDProcessorInstance">
		<xs:attribute name="id" type="xs:integer" use="required"/>
		<xs:attribute name="instance" use="required"/>
		<xs:attribute name="type" use="required"/>
		<xs:attribute name="plugin" use="required"/>
		<xs:attribute name="pluginVersion" use="required"/>
	</xs:complexType>
	<xs:complexType name="SEDProcessorSet">
		<xs:sequence>
			<xs:element name="SEDProcessorInstance" type="ns1:SEDProcessorInstance" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="code" type="xs:token" use="required"/>
		<xs:attribute name="name"/>
		<xs:attribute name="description"/>
	</xs:complexType>
	<xs:complexType name="SEDProcessorRule">
		<xs:attribute name="id" type="xs:integer" use="required"/>
		<xs:attribute name="procSetCode" type="xs:token" use="required">
			<xs:annotation>
				<xs:documentation>id to process set </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="service" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Storitve odpreme, ki jo določena v PMode parametrih </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="action" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Akcija v storitvi odpreme, ki jo določena v PMode parametrih  (npr: RegistredMail, LegalZPP, LegalZKP in LegalZUP, SMTPDelivery) </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="receiverEBox" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Naslovnikov eBox naslov (email oblika). domena določa PMode konfiguracijo naslovnikovega strežnika</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
					<xs:pattern value="[_\-a-zA-Z0-9\.\+]+@[a-zA-Z0-9](\.?[\-a-zA-Z0-9]*[a-zA-Z0-9])*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="senderEBox" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="sl">Pošiljateljev  elektronski naslov (email oblika). domena določa PMode konfiguracijo naslovnikovega strežnika</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:maxLength value="64"/>
					<xs:pattern value="[_\-a-zA-Z0-9\.\+]+@[a-zA-Z0-9](\.?[\-a-zA-Z0-9]*[a-zA-Z0-9])*"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="SEDProcessorSet" type="ns1:SEDProcessorSet"/>
	<xs:element name="SEDProcessorRule" type="ns1:SEDProcessorRule"/>
</xs:schema>
