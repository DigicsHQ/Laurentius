<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="si.laurentius.plugin.imp.IMPExecute" table="PLG_BSC_EXECUTE">
        <id column="Instance" name="Instance" type="java.lang.String" />
        <property column="EXECUTE_COMMAND" name="Command"  type="java.lang.String" length="512"  /> 
        <property column="EXECUTE_PARAMETERS" name="Parameters" type="java.lang.String" length="512"  />
    </class>
    <query name="si.laurentius.plugin.imp.IIMPExecute.getByInstance">SELECT p FROM IMPExecute p WHERE p.Instance = :instance</query>
    <query name="si.laurentius.plugin.imp.IMPExecute.getAll">SELECT p FROM IMPExecute p</query>
</hibernate-mapping>
